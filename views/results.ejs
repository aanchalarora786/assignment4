<!doctype html>
<html lang="en">

  <head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

    <title></title>
  </head>

  <body>
  // table 

    <table class="table" style="background-color:lightpink">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Seller</th>
            <th scope="col">Address</th>
            <th scope="col">City</th>
            <th scope="col">Phone</th>
            <th scope="col">Email</th>
            <th scope="col">Car Make</th>
            <th scope="col">Car Model</th>
            <th scope="col">car Year</th>
          </tr>
        </thead>
        <tbody>
        <% for(var i=0; i < texto.length; i++) { %>


          <!-- <tr>
             <td><%= texto[i].seller %></td>
             <td><%= texto[i].email %></td>
           </tr> -->

           <tr>
              <th scope="row"><%= i %></th>
              <td><%= texto[i].name %></td>
              <td><%= texto[i].address %></td>
              <td><%= texto[i].city %></td>
              <td><%= texto[i].phonenumber %></td>
              <td><%= texto[i].email %></td>
              <td>
                  <% var a %>
                  <% a = "http://www.jdpower.com/Cars/"+texto[i].make+"/"+texto[i].model+"/"+texto[i].year %>
                <a href=<%= a %>> <%= texto[i].make %> 
                </td>          
              <td><%= texto[i].model %></td>
              <td><%= texto[i].year %></td>
            </tr>


        <% } %>
      </tbody>
    </table>





        <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>
  </body>
  
</html>


   
